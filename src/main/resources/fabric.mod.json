{
  "schemaVersion": 1,
  "id": "meteor-moonware-addon",
  "version": "${version}",
  "name": "MoonWare Addon",
  "description": "MoonWare addon for the Meteor client",
  "authors": [
    "Moon_dark"
  ],
  "contact": {
    "repo": "https://github.com/Moondarker/moonware-meteor"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.moondark.moonware.Addon"
    ]
  },
  "mixins": [
    "moonware.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "0,160,224"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  },
  "suggests": {
    "litematica": ">=0.12.7"
  }
}
